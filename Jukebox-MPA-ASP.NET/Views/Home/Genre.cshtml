
@{
    ViewData["Title"] = "Genre-Songs";
    
}

<h1>Songs</h1>

@foreach(var front in ViewBag.item) 
{
 <div class = "songlist">
 <p>Song name @{@front.Name}</p>
 <p>Genre @{@front.Genre}</p>
 <button asp-controller="Home" asp-action = "Addtoplaylist(@{@front.id})" > Add to play list</Button>
 <button asp-controller="Home" asp-action="">Add to list</button>
 </div>
}
